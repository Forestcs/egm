 page {
  background-color: #fff;
} 

.logoImg{
  width: 188rpx;
  margin: 0 auto;
  margin-top: 150rpx;
}

.logoImg image{
  width: 188rpx;
  height: 188rpx;
}

.login-title {
  font-size: 36rpx;
  color: #666;
  padding: 82rpx 0 128rpx 0;
  text-align: center;
}

.login-title view {
  position: relative;
  top: -1rpx;
  display: inline-block;
  width: 14rpx;
  height: 14rpx;
  margin: 0 4rpx;
  background: #CFB680FF;
  border-radius: 15rpx;
}

.form-in {
  display: flex;
  justify-content: center;
  width: 80%;
  height: 96rpx;
  margin: 0 auto;
  margin-bottom: 28rpx;
  /* margin-left: -40rpx; */
  border: 2rpx solid #efefef;
  border-radius: 48rpx;
}

.username.password{
  position: absolute;
}

.form-in .icon{
  position: relative;
  top: 28rpx;
  /* left:42rpx; */
  width: 40rpx;
  height: 40rpx;
  margin-right:30rpx;
}

.form-in input {
  width: 75%;
  height: 96rpx;
  line-height: 33rpx;
}

.in-place {
  font-size: 32rpx;
  color: #CCCCCCFF;
}

.form-sub button{
  width: 80%;
  height: 96rpx;
  line-height: 96rpx;
  margin-top: 8rpx;
  margin-bottom: 36rpx;
  color: #fff;
  font-size: 36rpx;
  background: #CFB680FF;
  border-radius: 48rpx;
  -moz-box-shadow:2px 2px 10px #DDDD;
  -webkit-box-shadow:2px 2px 10px #DDDD;
  box-shadow:2px 2px 10px #DDDD;
}

.forget{
  width: 80%;
  margin:0 auto;
  font-size: 36rpx;
  color: #666666FF;
}

.forget text{
  float: right;
}